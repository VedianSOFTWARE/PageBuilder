{
    "name": "vediansoftware/vedian-cms",
    "description": "Content management system for laravel",
    "keywords": [
        "cms",
        "laravel",
        "vedian"
    ],
    "license": "MIT",
    "version": "1.0.0",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Vedian\\Cms\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "laravel/sail": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vedian\\Cms\\Providers\\CmsProvider"
            ],
            "aliases": {
                "Cms": "Vedian\\Cms\\Facades\\Cms",
                "Schema": "Vedian\\Cms\\Facades\\Schema"
            }
        }
    },
    "authors": [
        {
            "name": "Tom van Tilburg",
            "email": "tom@vediansoft.net"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}