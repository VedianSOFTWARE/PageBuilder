{
    "name": "vediansoftware/vedian-cms",
    "description": "Content management system for laravel",
    "type": "library",
    "autoload": {
        "psr-4": {
            "VedianSOFT\\CMS\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "laravel/framework": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "VedianSOFT\\CMS\\ServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Tom van Tilburg",
            "email": "tom@vediansoft.net"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}